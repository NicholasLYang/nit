datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String @id @default(cuid())
  email String
  githubLogin String
  githubId Int @unique

  recentlyVisitedRepositories String @default("")

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
